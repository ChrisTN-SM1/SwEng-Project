<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import org.kordamp.ikonli.javafx.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.ludina.bugboard26frontend.HomepageController">
    <TabPane layoutX="204.0" layoutY="26.0" prefHeight="622.0" prefWidth="1057.0" tabClosingPolicy="UNAVAILABLE">
        <Tab fx:id="issueListTab" text="Lista issue">
            <fx:include source="issue-list.fxml" />
        </Tab>
        <Tab fx:id="archiveTab" text="Archivio bug">
            <fx:include source="archive.fxml" />
        </Tab>
    </TabPane>
    <Button layoutX="14.0" layoutY="128.0" mnemonicParsing="false" onAction="#showCreateIssue" prefHeight="81.0" prefWidth="166.0" style="-fx-background-color: lightskyblue" text="Segnala Issue">
        <font>
          <Font size="22.0" />
        </font>
    </Button>
    <Button fx:id="createUserButton" layoutX="14.0" layoutY="222.0" mnemonicParsing="false" onAction="#showCreateUser" prefHeight="81.0" prefWidth="166.0" style="-fx-background-color: lightskyblue" text="Crea nuova&#10;Utenza" textAlignment="CENTER">
        <font>
          <Font size="22.0" />
        </font>
    </Button>
    <Label layoutX="78.0" layoutY="42.0" text="Aggiorna pagina">
        <font>
          <Font name="System Italic" size="12.0" />
        </font>
    </Label>
    <Button layoutX="29.0" layoutY="30.0" mnemonicParsing="false" onAction="#reloadPage" prefHeight="40.0" prefWidth="40.0" style="-fx-background-color: orange">
        <graphic>
            <FontIcon iconLiteral="mdi2r-reload" />
        </graphic>
    </Button>
   <Label layoutX="217.0" layoutY="648.0" text="*Visualizza filtri: click destro su colonna&#10;*Imposta ordine: click sinistro su colonna" />
</AnchorPane>
