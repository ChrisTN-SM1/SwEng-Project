<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.ludina.bugboard26.bugboard26frontend.HomepageController">
    <TabPane layoutX="204.0" layoutY="26.0" prefHeight="681.0" prefWidth="1057.0" tabClosingPolicy="UNAVAILABLE">
        <Tab text="Lista issue">
            <fx:include source="lista-issue.fxml" />
        </Tab>
        <Tab text="Archivio bug">
            <fx:include source="archivio.fxml" />
        </Tab>
    </TabPane>
    <Button layoutX="14.0" layoutY="128.0" mnemonicParsing="false" onAction="#mostraSegnalaIssue" prefHeight="81.0" prefWidth="166.0" style="-fx-background-color: lightskyblue" text="Segnala Issue">
        <font>
          <Font size="22.0" />
        </font>
    </Button>
    <Button layoutX="14.0" layoutY="222.0" mnemonicParsing="false" onAction="#mostraCreaNuovaUtenza" prefHeight="81.0" prefWidth="166.0" style="-fx-background-color: lightskyblue" text="Crea nuova&#10;Utenza" textAlignment="CENTER">
        <font>
          <Font size="22.0" />
        </font>
    </Button>
    <Label layoutX="78.0" layoutY="42.0" text="Aggiorna pagina">
        <font>
          <Font name="System Italic" size="12.0" />
        </font>
    </Label>
    <Button layoutX="29.0" layoutY="30.0" mnemonicParsing="false" onAction="#reloadPage" prefHeight="40.0" prefWidth="40.0" style="-fx-background-color: orange">
        <graphic>
            <!--FontIcon iconLiteral="mdi2r-reload" /-->
        </graphic>
    </Button>
</AnchorPane>
